{
    "plugins": ["react"],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "react/prop-types": "off",
        "semi": [2, "always"],
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": [2, { "max": 1 }],
        "no-undef": 0
    }
}